void pushZeroesEnd(int *input, int size)
{
    int i=0;
    int k=0;
    while(i<size)
    {
        if(input[i]!=0)
        {
            int temp=input[k];
            input[k]=input[i];
            input[i]=temp;
            i++;
            k++;
        }
        else
        {
            i++;
        }
    }
    //Write your code here
}
