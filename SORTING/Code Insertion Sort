void insertionSort(int *input, int size)
{
    for(int i=1;i<size;i++)
    {
        int current=input[i];
        int j=i-1;
        for(;j>=0;j--)
        {
            if(current<input[j])
            {
                input[j+1]=input[j];
            }
            else
            {
                break;
            }
        }
        input[j+1]=current;
    }
    //Write your code here
}
